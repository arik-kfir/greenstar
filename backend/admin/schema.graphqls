type Tenant {
    id: ID!
    displayName: String!
}

type Query {
    tenants: [Tenant!]!
    tenant(id: ID!): Tenant
}

input TenantChanges {
    displayName: String!
}

type Mutation {
    createTenant(tenantID: String, tenant: TenantChanges!): Tenant!
    updateTenant(tenantID: String!, tenant: TenantChanges!): Tenant!
    deleteTenant(tenantID: String!): ID!
}
